sandboxed: false
version: 0.1
start: manual

executables:
{
    bluetoothSensorTag = ( sensorTagComponent )
}

processes:
{
    run:
    {
        ( bluetoothSensorTag )
    }
    faultAction: stopApp
    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }
}

bindings:
{
    bluetoothSensorTag.sensorTagComponent.io -> dataHub.io
}
