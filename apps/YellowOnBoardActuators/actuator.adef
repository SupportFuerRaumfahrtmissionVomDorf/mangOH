sandboxed: false
start: auto
version: 0.1

executables:
{
    actuatorYellow = ( components/yellowActuators )
}

processes:
{
    run:
    {
        ( actuatorYellow )
    }

    envVars:
    {
        LE_LOG_LEVEL = DEBUG
    }
    faultAction: stopApp
}

bindings:
{
    actuatorYellow.outputActuator.dhub -> dataHub.io
}
